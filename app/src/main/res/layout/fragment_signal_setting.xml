<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whiteTwo">

    <LinearLayout
        android:id="@+id/layout_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/layout_signal_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/tag_name_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="M1"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_4" />

            <EditText
                android:id="@+id/edit_set_1"
                style="@style/edit_signal_params_set"
                android:layout_marginStart="10dp"
                android:text="999" />

            <TextView
                android:id="@+id/tv_start_value_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="1"
                android:textColor="@color/warmGrey"
                android:textSize="@dimen/text_size_7" />

            <android.support.v7.widget.AppCompatSeekBar
                android:id="@+id/seek_bar_1"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="3dp"
                android:layout_marginStart="3dp"
                android:focusable="true"
                android:max="100"
                android:maxHeight="3dp"
                android:minHeight="3dp"
                android:paddingEnd="0dp"
                android:paddingStart="0dp"
                android:progress="0"
                android:progressDrawable="@drawable/signal_params_set_seekbar"
                android:secondaryProgress="0"
                android:thumb="@drawable/signal_params_set_seekbar_thumb"
                android:thumbOffset="0dp" />

            <TextView
                android:id="@+id/tv_end_value_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="100"
                android:textColor="@color/warmGrey"
                android:textSize="@dimen/text_size_7" />
        </LinearLayout>

